        "string_view": "cpp",
        "*.tcc": "cpp",
        "new": "cpp",
        "semaphore": "cpp",
        "span": "cpp",
        "variant": "cpp"
    },
    "clangd.arguments": [
        "--query-driver=D:/CppMinGW/mingw64/bin/g*",
    ],
